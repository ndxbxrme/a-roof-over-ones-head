.angular-google-map-container
  height 400px
  
.icon
  border 2px solid black
  
.greyed-out
  opacity 0.6
  
.selected-image
  background rgba(0,0,0,0.7)
  min-height 400px
  img
    max-width 100%
    cursor pointer
  .close-btn
    position absolute
    top 0
    right 20px
    color #fff
    font-size 2rem
    
.margin-bottom
  margin-bottom 10px
  
.header-placeholder
  height 70px
  
.search-listings
  padding-top 20px
  clear both
  
  img
    max-width 100%
    cursor pointer
    
.scroller 
  white-space pre-line
  height 400px
  overflow-y hidden
  position relative

.selected-property
  img
    max-width 100%
    cursor pointer